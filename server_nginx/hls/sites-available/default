server {
	listen 80 default_server;
	listen [::]:80 default_server;

	root /var/www/html;

	index index.html index.htm index.nginx-debian.html;

	server_name NovaGenerator;

	location / {
		try_files $uri $uri/ =404;
	}

	location /hls/ {
		root /var/www;
		add_header Cache-Control no-cache;
		add_header Access-Control-Allow-Origin *;
	}
}

